{
  "rules": {
    "no-console": "off",
    "max-len":  ["error", { "code": 150 }]
    },
  "env": {
      es6: true,
      node: true,
      mocha: true
  },
  "plugins": [
      "json",
      "mocha"
  ],
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "extends": "airbnb-base"
}
